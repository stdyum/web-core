{
  "name": "@likdan/studyum-core",
  "description": "core library for studyum",
  "author": {
    "name": "LikDan",
    "email": "likdan.official@gmail.com",
    "url": "https://github.com/stdyum"
  },
  "version": "0.0.21",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:app && npm run build:styles",
    "build:app": "ng build && npm run build:styles",
    "build:styles": "sass --no-source-map -I node_modules public/styles.scss dist/public/styles.css",
    "watch": "npm run watch:app & npm run watch:styles",
    "watch:app": "ng build --watch",
    "watch:styles": "sass --no-source-map --watch -I node_modules public/styles.scss dist/public/styles.css"
  },
  "peerDependencies": {
    "@angular/cdk": "^18.1.0-next.3",
    "@angular/common": "^18.1.0-next.3",
    "@angular/compiler": "^18.1.0-next.3",

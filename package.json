    "@angular/forms": "^18.1.0-next.3",
    "@angular/material": "^18.1.0-next.3",
    "@angular/material-experimental": "^18.1.0-next.3",
    "@angular/router": "^18.1.0-next.3",
    "@likdan/form-builder-core": "0.0.11",
    "@likdan/form-builder-material": "0.0.15",
    "@likdan/state-mapper": "^0.0.2",
    "rxjs": "~7.8.1",
    "zone.js": "^0.14.4"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0-next.3",
    "@angular/cli": "^18.1.0-next.3",
